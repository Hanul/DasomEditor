{
	"name": "skyeditor",
	"version": "0.0.1",
	"main": "main.js",
	"dependencies": {
		"bowser": "^2.10.0",
		"rimraf": "latest",
		"uppercase": "^2.0.0-beta.16",
		"uppercase-core": "^2.0.0-beta.17"
	},
	"scripts": {
		"package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=favicon.ico --prune=true --out=release-builds --electronVersion=9.2.0",
		"package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=favicon.ico --prune=true --out=release-builds --electronVersion=9.2.0",
		"package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=favicon.ico --prune=true --out=release-builds --electronVersion=9.2.0"
	}
}
